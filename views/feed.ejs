<%- include('partials/header') %>
    <h1>pins</h1>
    <div class="cards">
        <% post.forEach(elem => { %>
            <div class="card">
                <div style="overflow: hidden; width: 200px; height: 300px;">
                    <img src="/images/uploads/<%=elem.image%>" alt="" style="width: 100%; height: 100%;">
                </div>
                <h3><%= elem.title %></h3>
                <h4>@<%= elem.user.username %></h4>
            </div>
            
        <% }); %>
    </div>
<%- include('partials/footer') %>